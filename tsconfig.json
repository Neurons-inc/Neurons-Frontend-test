{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~*": [
        "src/*"
      ]
    },
    "target": "ES2017",
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
  }
}
